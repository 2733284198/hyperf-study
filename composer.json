{
    "name": "hyperflex/hyperflex-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperflex",
        "microservice",
        "middleware"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.1",
        "ext-swoole": ">=4.2",
        "hyperflex/hyperflex": "dev-master",
        "hyperflex/config": "dev-master",
        "hyperflex/di": "dev-master",
        "hyperflex/http-server": "dev-master",
        "hyperflex/dispatcher": "dev-master",
        "hyperflex/memory": "dev-master",
        "doctrine/common": "^2.9",
        "swoft/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "hyperflex/coding-standard": "~1.0.0",
        "symfony/console": "^4.1",
        "php-di/php-di": "^6.0",
        "swoft/swoole-ide-helper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperflexTest\\Dispatcher\\": "./vendor/hyperflex/dispatcher/test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "./vendor/bin/phpunit -c phpunit.xml",
        "start": "php ./bin/hyperflex.php start"
    },
    "repositories": {
        "0": {
            "type": "path",
            "url": "../hyperflex-components/hyperflex"
        },
        "1": {
            "type": "path",
            "url": "../hyperflex-components/config"
        },
        "2": {
            "type": "path",
            "url": "../hyperflex-components/utils"
        },
        "3": {
            "type": "path",
            "url": "../hyperflex-components/di"
        },
        "4": {
            "type": "path",
            "url": "../hyperflex-components/http-server"
        },
        "5": {
            "type": "path",
            "url": "../hyperflex-components/dispatcher"
        },
        "6": {
            "type": "path",
            "url": "../hyperflex-components/memory"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
