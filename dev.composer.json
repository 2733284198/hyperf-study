{
    "name": "hyperf/hyperf-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.3.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "ext-protobuf": "*",
        "hyperf/hyperf": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0",
        "symfony/console": "^4.1",
        "swoft/swoole-ide-helper": "^4.3",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "guzzlehttp/guzzle": "^6.3",
        "mockery/mockery": "^1.0",
        "doctrine/common": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "GPBMetadata\\": "grpc/GPBMetadata",
            "Grpc\\": "grpc/Grpc"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "./test/",
            "HyperfTest\\Consul\\": "./vendor/hyperf/hyperf/src/consul/tests/",
            "HyperfTest\\Dispatcher\\": "./vendor/hyperf/hyperf/src/dispatcher/test/",
            "HyperfTest\\Database\\": "./vendor/hyperf/hyperf/src/database/tests/",
            "HyperfTest\\Event\\": "./vendor/hyperf/hyperf/src/event/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "./vendor/bin/phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "start": "php ./bin/hyperf.php start"
    },
    "repositories": {
        "hyperf": {
            "type": "path",
            "url": "../hyperf"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.laravel-china.org"
        }
    }
}
